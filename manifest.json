{
  "manifest_version": 2,
  "name": "STEEM Start Menu",
  "version": "0.1",
  "description": "A menu of shortcuts for STEEM blockchain apps.",


  "permissions": [
    "activeTab"
  ],

  "browser_action": {
    "default_title": "Start",
    "default_popup": "popup.html",
    "browser_style": true
  },

  "background": {
    "scripts": ["background.js"]
  }
}